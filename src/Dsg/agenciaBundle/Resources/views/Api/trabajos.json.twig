{% for url, oferta in trabajos %}
{% i = 0, count(trabajos), ++i %}
[
    "url":{{ url }},
{% for key, value in oferta %} {% j = 0, count(key), ++j %}
    {{ key }}:"{% if j == count(key)%} {{ json_encode(value) }}, {% else %} {{ json_encode(value) }}
                 {% endif %}"
{% endfor %}]
{% endfor %}